{
  "name": "cliqpat-backend",
  "version": "1.0.0",
  "description": "Backend server for Cliqpat healthcare management system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "clean-start": "powershell -Command \"Get-Process | Where-Object {$_.ProcessName -eq 'node'} | Stop-Process -Force; npm run dev\"",
    "force-start": "powershell -ExecutionPolicy Bypass -File start-server.ps1",
    "kill": "powershell -Command \"Get-Process | Where-Object {$_.ProcessName -eq 'node'} | Stop-Process -Force\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["healthcare", "appointments", "mongodb", "express"],
  "author": "Cliqpat Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
